{
  "hosting": {
    "site": "foranimals",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*", 
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/public/prelaunch.html"
      },{
        "source": "**",
        "destination": "/public/prelaunch.html"
      }
    ]
  }
}

